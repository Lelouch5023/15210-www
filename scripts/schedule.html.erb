  <!-- generated by scripts/make_schedule.rb from data in scripts/schedule.yaml -->
  <!-- DO NOT make changes to this file. They will be OVERWRITTEN! -->
  <ol id="schedule"><%
  @schedule.each_with_index do |week, week_num| %>
    <li class="week">
      <h3>Week <%= week_num + 1 %></h3>
      <dl class="events"><%
        DAYS.each_with_index do |d, d_idx|
          next if week[d].nil? %>
          <dt><%= (@day+d_idx).strftime("%b %-d") %></dt><%
          week[d].each do |event| %>
            <dd><%= label(event) %> <%= topic(event)
            %><% event["links"] && event["links"].each do |link| %>
              &middot; <a class="link" href="<%= link["url"] %>"><%= link["title"] %></a><%
            end %></dd><%
          end %><%
        end %>
      </dl>
    </li><%
    @day += DAYS.length
  end %>
  </ol>
